{
    "name": "Znittzel/Zepository",
    "type": "library",
    "description": ":A simple resource and repository module for easy going api's.",
    "keywords": [
        "Znittzel",
        "Zepository"
    ],
    "homepage": "https://github.com/znittzel/zepository",
    "license": "MIT",
    "authors": [
        {
            "name": ":Rikard Olsson",
            "email": ":rikard@thryselius.se",
            "homepage": ":https://www.thryselius.se",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0",
        "vinelab/neoeloquent": "1.4.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Znittzel\\Zepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Znittzel\\Zepository\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
